<style>
    .terminal {
        font-family: monospace;
        background-color: #000;
        color: #fff;
        padding: 20px;
        white-space: pre-wrap;
        font-size: 10px;
    }
</style>
<div class="mb-3 row">
  <% if job_execution.execution_started_at.present? %>
    <div class="col-sm-12 terminal p-3"><%= fetch_log_events(job_execution) %></div>
  <% else %>
    <div class="col-sm-12">
      <h6>No logs found!</h6>
    </div>
  <% end %>
</div>


